{
  "name": "wbcodes/site-core",
  "description": "This websites site core console commands package",
  "license": "MIT",
  "authors": [
    {
      "name": "Zahir Hayrullah",
      "email": "zahir.hayrullah@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/support": "^7.3|^8.0",
    "laravel/scout": "^8.6.0",
    "predis/predis": "^1.1.7",
    "spatie/laravel-permission": "^4.4.0",
    "yajra/laravel-datatables-oracle": "^9.18.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.8"
  },
  "autoload": {
    "psr-4": {
      "Wbcodes\\SiteCore\\": "src/"
    },
    "files": [
      "src/Helpers/Functions/general.php",
      "src/Helpers/Functions/cache.php",
      "src/Helpers/Functions/files.php",
      "src/Helpers/Functions/site_helper.php"
    ],
