  "autoload-dev": {
    "psr-4": {
      "Wbcodes\\SiteCore\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "\\Wbcodes\\SiteCore\\Providers\\SiteCoreServiceProvider"
      ]
    }
  }
}
